@model string
@inject UserManager<AppUser> UserManager

<a asp-page="edituser" asp-route-id="@Model" class="btn btn-sm btn-secondary">Edit</a>

@if (UserManager.SupportsUserClaim)
{
    <a asp-page="claims" asp-route-id="@Model" class="btn btn-sm btn-info">Claims</a>
}

@if (UserManager.SupportsUserRole)
{
    <a asp-page="userroles" asp-route-id="@Model" class="btn btn-sm btn-info">
        Roles
    </a>
}

@if (UserManager.SupportsUserAuthenticatorKey)
{
    <a class="btn btn-sm btn-success" asp-page="AuthenticatorSetup" asp-route-id="@Model">
        Authenticator
    </a>
}